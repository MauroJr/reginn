{
  "private": true,
  "name": "reginn",
  "version": "0.0.0",
  "description": "Composable Command Line Applications",
  "main": "packages/reginn/dist/reginn.js",
  "files": [ "lib", "src", "type-definitions" ],
  "scripts": {
    "clean": "northbrook exec -- ../../node_modules/.bin/rimraf dist node_modules && rimraf node_modules",
    "build:rollup": "northbrook exec -- ../../node_modules/.bin/rollup -c",
    "build": "northbrook link && npm run build:rollup",
    "test:lint": "northbrook eslint",
    "test:unit": "mocha -r buba/register",
    "test": "npm run test:lint && npm run test:unit",
    "commit": "northbrook commit",
    "release": "northbrook release",
    "preversion": "npm run build",
    "postinstall": "northbrook exec --exclude packages/help,packages/reginn -- npm install && npm run build"
  },
  "repository": { "type": "git", "url": "git+https://github.com/northbrookjs/reginn.git" },
  "keywords": [
    "northbrook",
    "command",
    "line",
    "application",
    "cli",
    "framework",
    "function",
    "functional",
    "reactive",
    "observable",
    "most.js",
    "reginn"
  ],
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "bugs": { "url": "https://github.com/northbrookjs/reginn/issues" },
  "homepage": "https://github.com/northbrookjs/reginn#readme",
  "config": {
    "ghooks": { "commit-msg": "node ./node_modules/.bin/validate-commit-msg" },
    "validate-commit-msg": { "types": "@northbrook/commit-types" }
  },
  "devDependencies": {
    "@northbrook/eslint": "^1.2.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-power-assert": "^1.0.0",
    "buba": "^3.0.1",
    "eslint-plugin-import": "^1.15.0",
    "mocha": "^3.0.2",
    "northbrook": "^3.1.3",
    "power-assert": "^1.4.1",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.0",
    "rollup-plugin-buba": "^1.0.1",
    "sinon": "^1.17.6"
  }
}
